  "license": "ISC",
  "scripts": {
    "build": "next build",
    "clean": "rimraf .next",
    "custom": "NODE_ENV=development node --inspect --max-old-space-size=512 --trace-gc src/server.js",
    "debug": "ANALYZE=true NODE_OPTIONS='--inspect' next dev",
    "dev": "next",
    "eslint": "npx eslint src --ext .ts,.tsx",
    "eslint:debug": "DEBUG=eslint:cli-engine npx eslint src --ext .ts,.tsx",
    "eslint:fix": "npx eslint src --ext .ts,.tsx --fix",
    "export": "next export",
    "start": "next start",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:debug": "node --inspect-brk ../../node_modules/.bin/jest --runInBand",
    "update": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@try-govuk-react-jsx-next/ui-components": "^0.0.1",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
