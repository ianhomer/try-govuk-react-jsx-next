    "@types/react-helmet": "^6.1.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "inspectpack": "^4.5.2",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.6.2",
    "next-transpile-modules": "^6.0.0"
  }
