    "govuk-frontend": "^3.10.2",
    "govuk-react-jsx": "^4.1.0",
    "next": "^10.0.4",
    "node-sass": "4.14.1",
    "null-loader": "^4.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^17.0.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@next/bundle-analyzer": "^10.0.4",
