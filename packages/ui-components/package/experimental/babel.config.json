{
  "presets": [
    [
      "@babel/preset-env",
      {
        "exclude": ["@babel/plugin-proposal-dynamic-import"]
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-transform-runtime",
    [
      "@babel/plugin-transform-typescript",
      {
        "isTSX": true,
        "allExtensions": true
      }
    ]
  ]
}
